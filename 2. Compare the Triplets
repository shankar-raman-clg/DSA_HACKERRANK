#include <stdio.h>
#include <stdlib.h>

int* compareTriplets(int a_count, int* a, int b_count, int* b, int* result_count) {
    *result_count = 2;
    int* results = (int*)malloc(2 * sizeof(int));
    
    results[0] = 0; // Alice
    results[1] = 0; // Bob
    
    for (int i = 0; i < 3; i++) {
        if (a[i] > b[i]) results[0]++;
        else if (a[i] < b[i]) results[1]++;
    }
    return results;
}
