#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char* timeConversion(char* s) {
    int h1 = s[0] - '0';
    int h2 = s[1] - '0';
    int hh = h1 * 10 + h2;
    char am_pm = s[8];

    if (am_pm == 'A') {
        if (hh == 12) hh = 0;
    } else {
        if (hh != 12) hh += 12;
    }

    char* result = (char*)malloc(9 * sizeof(char));
    sprintf(result, "%02d:%c%c:%c%c", hh, s[3], s[4], s[6], s[7]);
    return result;
}
